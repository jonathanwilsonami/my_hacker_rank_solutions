N = int(input())
S = []
myWords = []

for s in range(N):
    #Build a list of words and make words (inputs) into a list of chars
    S.append(list(input()))
    #Recall my_list[start:stop:step] can return even and odd indeces
    even = ''.join(S[s][1::2])
    odd = ''.join(S[s][0::2])
    print(odd+' '+even)
